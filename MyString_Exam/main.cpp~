{
  MyString str1 = "Aha!";
  MyString str2 = "My friend";
  MyString str3;
  if (!(str1 == str2))
    cout << str1 << " " << str2 << endl;
  cout << str1.size() << " " << str2.size() << endl;
  cout << str1[str1.size()] << endl;
}
